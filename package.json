{
    "name": "@mpiorowski/svelte-init",
    "description": "Low-overhead Svelte UI library",
    "keywords": [
        "svelte",
        "sveltekit",
        "svelteui",
        "ui",
        "components",
        "library"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/mpiorowski/svelte-init"
    },
    "homepage": "https://svelte-init.vercel.app",
    "license": "MIT",
    "version": "1.4.6",
    "type": "module",
    "scripts": {
        "start": "vite dev",
        "build": "vite build",
        "package": "svelte-kit sync && svelte-package && cp ./src/styles.css ./package/styles.css && cp ./src/preflight.css ./package/preflight.css",
        "test": "playwright test",
        "check": "svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
        "lint": "svelte-check --tsconfig ./tsconfig.json && eslint src"
    },
    "exports": {
        "./styles.css": "./styles.css"
    },
    "devDependencies": {
        "@playwright/test": "1.27.1",
        "@sveltejs/adapter-auto": "1.0.0-next.87",
        "@sveltejs/kit": "1.0.0-next.535",
        "@sveltejs/package": "1.0.0-next.5",
        "@typescript-eslint/eslint-plugin": "5.42.0",
        "@typescript-eslint/parser": "5.42.0",
        "eslint": "8.27.0",
        "eslint-plugin-svelte3": "4.0.0",
        "svelte": "3.52.0",
        "svelte-check": "2.9.2",
        "svelte-preprocess": "4.10.7",
        "tslib": "2.4.1",
        "typescript": "4.8.4",
        "vite": "3.2.2"
    }
}
